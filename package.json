{
  "name": "gmail-calendar-assistant",
  "version": "1.0.0",
  "description": "AI-powered calendar assistant for sales demo scheduling via Gmail and Google Calendar integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/server/index.ts",
    "dev:frontend": "next dev frontend",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc && tsc-alias",
    "build:frontend": "next build frontend",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts frontend/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.ts frontend/**/*.{ts,tsx} --fix",
    "typecheck": "tsc --noEmit && cd frontend && npx tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx src/database/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:test": "tsx scripts/test-database.ts",
    "api:test": "tsx scripts/test-api-integration.ts",
    "test:full": "npm run db:test && npm run api:test",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": ["gmail", "calendar", "assistant", "ai", "scheduling", "demo"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "googleapis": "^128.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "openai": "^4.20.1",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "axios": "^1.6.2",
    "@prisma/client": "^5.6.0",
    "bcrypt": "^5.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.2",
    "typescript": "^5.2.2",
    "tsc-alias": "^1.8.8",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.3.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "prisma": "^5.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.7"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}